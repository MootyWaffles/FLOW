//Specify the typedef
use "Depth.flowt";

int depth(Node myNode)
{
	int deep = 0;
	List of Arc arcs;
	arcs = myNode.arcsOut;
	if(arcs.length == 0) {
		return 0;
	}
	int k = 0;
	while(k < arcs.length) {
		Arc n = arcs[k];
		int temp = depth(n.to);
		if(temp > deep) {
			deep = temp;
		}
	}
	return (deep + 1);
}
print depth(Graph.ROOT);
